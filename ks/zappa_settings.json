{
    "dev": {
        "app_function": "app.app",
        "aws_region": "us-east-1",
        "profile_name": "default",
        "project_name": "ks",
        "runtime": "python3.7",
        "s3_bucket": "yourname-watermark",
        "keep_warm": false,
        "extra_permissions": [{
            "Effect": "Allow",
            "Action": ["cloudwatch:*"],
            "Resource": "*"
        }],
        "environment_variables": {
            "JWT_SECRET": "my_secret"
        }
    }
}